    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@release-it/conventional-changelog": "^10.0.1",
        "@types/fs-extra": "^11.0.4",
        "@types/prompts": "^2.4.9",
        "conventional-changelog-cli": "^5.0.0",
        "eslint": "^9.29.0",
        "globals": "^16.2.0",
        "prettier": "^3.5.3",
        "release-it": "^19.0.3",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.0"
