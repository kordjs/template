{
    "name": "create-kordjs",
    "version": "1.1.2",
    "description": "For kordjs (dev)",
    "main": "dist/index.js",
    "bin": "bin/index.js",
    "publishConfig": {
        "access": "public"
    },
    "directories": {
        "bin": "bin",
        "lib": "src"
    },
    "files": [
        "templates/",
